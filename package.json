{
  "name": "pollaroid",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "yarn && tsc",
    "start": "yarn deploy-commands && node dist/index.js",
    "deploy-commands": "tsx src/deploy-commands.ts",
    "lint": "eslint src/",
    "format:check": "prettier --check src/",
    "format": "prettier --write src/",
    "typecheck": "tsc --noEmit",
    "validate": "yarn typecheck && yarn lint && yarn format:check"
  },
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "discord.js": "^14.25.1",
    "dotenv": "^17.3.1",
    "knex": "^3.1.0",
    "lodash-es": "^4.17.23",
    "nanoid": "^5.1.6",
    "pg": "^8.19.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.3.0",
    "eslint": "^10.0.1",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  }
}
